{
  "window.title": "@vscode/nickkelly.dev",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "eslint.lintTask.options": "-c .eslintrc.js .",
  "eslint.lintTask.enable": false,
  "files.associations": {
    "*.md2": "markdown",
  },
}
